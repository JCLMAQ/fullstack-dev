API_BACKEND_URL=http://${API_BACKEND}:${API_BACKEND_PORT}/${API_BACKEND_PREFIX}
# API_BACKEND_PREFIX=${NEST_SERVER_GLOBAL_PREFIX}
API_BACKEND_PREFIX=api

# API Secret and key
