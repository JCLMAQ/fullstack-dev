FILES_MAX_SIZE=1e7
IMAGES_STORAGE_DEST=./files/uploadedimages
IMAGES_TEMP_STORAGES_DEST=./uploadedtempimages
# IMAGES_EXTENSIONS_REGEX =\/(jpe?g|png|gif|tiff)\$
IMAGES_MAX_SIZE=1e7
IMAGES_SIZING=25X25,50X50,100X100,200X200,400X400,900X900
