{
  "servers": {
    "angular-cli": {
      "command": "npx",
        "args": ["-y", "@angular/cli", "mcp", "-E", "modernize", "-E", "ai-tutor"]
    },
    "Prisma-Remote": {
        "command": "npx",
        "args": ["-y", "mcp-remote", "https://mcp.prisma.io/mcp"]
    },
    "deepcontext": {
      "command": "npx",
      "args": [
        "-y",
        "@wildcard-ai/deepcontext"
      ]
    },
    "github": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/"
    }
  }
}



