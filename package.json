  "scripts": {
    "start-nest-app": "nx serve nest-app --watch",
    "start-dev-app": "nx serve dev-app --watch",
    "start:frontend:dev": "pnpm run configangular -- --environment=dev && nx serve dev-app --proxy-config ./proxy.config.json --host 127.0.0.1 --port 4200",
    "dep:cycles": "npx -y madge --circular --extensions ts,tsx --ts-config tsconfig.base.json apps libs",
    "pnpm-instal": "pnpm install --no-frozen-lockfile",
    "pnpm-auto-update": "pnpm update",
    "pnpm approve-builds scripts": "pnpm approve-builds",
    "nxupdate": "nx migrate latest",
    "nxmigrate": "nx migrate --run-migrations",
    "start:prisma": "pnpm run prisma:generate && pnpm run prisma-migrate",
    "prisma:generate": "pnpm exec prisma generate",
    "prisma:generate:watch": "pnpm exec prisma generate --watch",
    "prisma-migrate": "pnpm exec prisma migrate dev ",
    "configangular": "npx tsx ./scripts/setenv.ts",
    "configproxy": "dotenv -e ../.env ts-node ./scripts/setproxyconfig.ts",
    "debug nestjs": "nx run-many -t typecheck"
  },
