{
  "name": "@fullstack-dev/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:backend:dev": "pnpm run configproxy && nx serve nest-app --watch",
    "start:frontend:dev": "pnpm run configangular -- --environment=dev && pnpm run update-project-port && dotenvx run -- nx serve app-jcm --proxy-config ./proxy.config.json",
    "start:prisma": "pnpm run prisma:generate && pnpm run prisma-migrate",
    "prisma:studio": "dotenvx run prisma studio",
    "pnpm-install": "pnpm install --no-frozen-lockfile",
    "pnpm-auto-update": "pnpm update",
    "pnpm-approve-builds-scripts": "pnpm approve-builds",
    "nxupdate": "nx migrate latest",
    "nxmigrate": "nx migrate --run-migrations",
    "db:docker:up": "dotenvx run -- docker compose -f './docker/docker-compose.yml' up -d --build",
    "db:docker:down": "dotenvx run -- docker compose -f './docker/docker-compose.yml' down",
    "prisma:generate": "dotenvx run -- pnpm exec prisma generate",
    "prisma:generate:watch": "dotenvx run -- pnpm exec prisma generate --watch",
    "prisma-migrate": "dotenvx run -- pnpm exec prisma migrate dev",
    "prisma-reset": "pnpm exec prisma migrate reset",
    "configangular": "dotenvx run -- npx tsx ./scripts/setenv.ts",
    "update-project-port": "dotenvx run -- npx tsx ./scripts/update-project-port.ts",
    "configproxy": "dotenvx run -- ts-node ./scripts/setproxyconfig.ts"
  },
  "private": true,
